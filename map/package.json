    "docs:build-ci": "npm install --registry=https://dolphin-dev.kedacom.com/ty-knpm && vuepress build docs && npm run copy",
    "start": "cross-env TS_NODE_DEBUG=false NODE_ENV=dev TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack-dev-server --mode=development",
